{
    "name": "etsglobal/log-bundle",
    "type": "symfony-bundle",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^1.24 || ^2.0",
        "symfony/config": "^3.4 || ^4.0 || ^5.0",
        "symfony/console": "^3.4 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.0 || ^5.0",
        "symfony/monolog-bundle": "^3.3"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.4",
        "psr/http-message": "^1.0",
        "slevomat/coding-standard": "^6.4 || ^7.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/http-client-contracts": "^2.1"
    },
    "suggest": {
        "csa/guzzle-bundle": "Allow token forwarding through Guzzle HTTP calls",
        "symfony/http-client": "Allow token forwarding through Symfony's HttpClient HTTP calls"
    },
    "autoload": {
        "psr-4": {
            "ETSGlobal\\LogBundle\\": "\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ETSGlobal\\LogBundle\\": "Tests\\"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
